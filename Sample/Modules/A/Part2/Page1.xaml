<UserControl x:Class="PageNavigatorSample.Modules.A.Part2.Page1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="Page Navigator Sample" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Margin="0 0 0 36" />
        <TextBlock Text="Module A" Foreground="WhiteSmoke" FontWeight="Bold" HorizontalAlignment="Center"  />
        <TextBlock Text="Part 2" Foreground="WhiteSmoke" FontWeight="Bold" HorizontalAlignment="Center"  />
        <TextBlock Text="Page 1" Foreground="WhiteSmoke" FontWeight="Bold" HorizontalAlignment="Center"  />
        
        <TextBlock HorizontalAlignment="Center" Margin="0 36 0 0">
            <Hyperlink Foreground="White" Command="{Binding NavigateCommand}">
                Go to page2
            </Hyperlink>
        </TextBlock>
    </StackPanel>
</UserControl>
